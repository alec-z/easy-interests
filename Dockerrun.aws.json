{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions":  [
    {
      "name": "web",
      "image": "aleczheng/interests-front",
      "hostname": "web",
      "essential":  true,
      "links": ["backend"],
      "memory": 128,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "environment": [
        {
          "name": "NGINX_BACK_END",
          "value": "backend"
        }
      ]
    },
    {
      "name": "backend",
      "image": "aleczheng/interests-back",
      "hostname": "backend",
      "memory": 128,
      "essential": false
    }
  ]
}
